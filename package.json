{
    "name": "flowerpiece",
    "version": "0.5.0",
    "description": "A Rich Text Data Model",
    "license": "MIT",
    "keywords": [],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "author": "lei chen <lei91122@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/Basaltic/flowerpiece.git"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "tsup src/index.ts",
