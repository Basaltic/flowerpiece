        "jsdom": "^22.1.0",
        "lint-staged": "^13.2.3",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "tsup": "^7.1.0",
        "typescript": "^5.1.6",
        "vitest": "^0.32.4"
    },
    "dependencies": {
        "immer": "^10.0.2",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isequal": "^4.5.0",
